@using FOAEA3.Model

@model ApplicationData

@{ 
    bool coreDisabled = ViewBag.CoreDisabled;
    Dictionary<string, string> genderChoices = ViewBag.GenderChoices;
    string tablePrefix = ViewBag.TablePrefix;
}

<flas-input table-prefix="@tablePrefix" asp-for="Appl_Dbtr_SurNme" form-id="[04]" size="5" required disabled="coreDisabled"></flas-input>
<flas-input table-prefix="@tablePrefix" asp-for="Appl_Dbtr_FrstNme" form-id="[05]" size="5" required disabled="coreDisabled"></flas-input>
<flas-input table-prefix="@tablePrefix" asp-for="Appl_Dbtr_MddleNme" form-id="[06]" size="5" disabled="coreDisabled"></flas-input>
<flas-input table-prefix="@tablePrefix" asp-for="Appl_Dbtr_Brth_Dte" form-id="[07]" size="5" placeholder="YYYY/MM/DD" required disabled="coreDisabled"></flas-input>
<flas-toggle table-prefix="@tablePrefix" asp-for="Appl_Dbtr_Gendr_Cd" form-id="[08]" size="3" single-line values="genderChoices" required disabled="coreDisabled"></flas-toggle>
<flas-input table-prefix="@tablePrefix" asp-for="Appl_Dbtr_Entrd_SIN" form-id="[09]" size="5" disabled="coreDisabled"></flas-input>
<flas-input table-prefix="@tablePrefix" asp-for="Appl_Dbtr_Parent_SurNme" form-id="[10]" size="5" disabled="coreDisabled"></flas-input>

